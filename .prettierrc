{
  "semi": false,
  "singleQuote": true,
  "trailingComma": "none",
  "arrowParens": "avoid",
  "printWidth": 100,

  "importOrder": ["^@/", "^\\."],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,

  "parser": "babel-ts",
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
